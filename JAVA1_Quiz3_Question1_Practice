public class Quiz3_Q1 {
  
  public static void main(String[] args) {
    printChars('c', 20, 6);
  }
  public static void printChars(char ch1, int numberofChars, int numbersperLine){
    // loop through and print 20 characters
    int counter = 0;
    for (int i = ch1; i < ch1 + numberofChars; i++){
      // print spaces between characters. Typecast (char) so that i = characters and it doesn't print numbers
      System.out.print(" " + (char)(i));
      // every time it prints, add 1 to the counter
      counter++;
      // once counter is divisible by 6 (numbersperLine), print a new line. 
      if (counter % numbersperLine == 0){
        System.out.println();
      }
    }
  }
}
